[user]
	name = Rasmy Nguyen
	email = rtnguyen87@gmail.com
[core]
	editor = nvim
	excludesFile = ~/.config/git/.gitignore
[color]
	ui = true
[alias]
	logg = log --oneline --graph --decorate --all --abbrev-commit
	co = checkout
	ci = commit
	br = branch
	df = diff
	dfs = diff --staged
	st = status
	sw = switch
	re = restore
[credential]
	helper = osxkeychain
[init]
	defaultBranch = main
[pull]
	rebase = false
[include]
	path = ~/.config/git/.privateconfig
